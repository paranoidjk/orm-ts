{
  "name": "orm-ts",
  "version": "0.1.11",
  "description": "orm",
  "keywords": [
    "orm"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "dist"
  ],
  "dependencies": {
    "@types/node": "^9.4.7",
    "any-cache": "^0.0.5",
    "autobind-decorator": "^2.1.0",
    "power-di": "^1.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "coveralls": "^3.0.0",
    "nyc": "^11.4.1",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "rimraf -- lib/**/*.js lib/**/*.js.map lib/**/*.d.ts",
    "cov": "egg-bin cov",
    "lint": "tslint -p .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "build:prod": "tsc && babel lib --out-dir lib",
    "build:dist": "webpack --progress",
    "test": "npm run lint -- --fix && npm run test-local",
    "test:prod": "npm run build:prod && ava -v -s",
    "test-local": "tsc && ava -v -s",
    "coverage": "nyc npm test",
    "coverage:report": "nyc report --reporter=lcov",
    "coverage:upload": "cat ./coverage/lcov.info | coveralls",
    "report": "npm run coverage && npm run coverage:report",
    "prepublish": "npm run clean && npm run test",
    "postpublish": "node scripts/published.js"
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  },
  "ci": {
    "type": "travis",
    "version": "6, 7",
    "license": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhang740/egg-cache.git"
  },
  "bugs": {
    "url": "https://github.com/zhang740/egg-cache/issues"
  },
  "homepage": "https://github.com/zhang740/egg-cache#readme",
  "author": "zhang740@vmzclub.com",
  "license": "MIT"
}
